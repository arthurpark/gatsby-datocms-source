{
  "name": "gatsby-datocms-source",
  "version": "0.2.8",
  "description": "GraphQL data source for GatsbyJS 1.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/datocms/gatsby-datocms-source.git"
  },
  "scripts": {
    "lint:autocorrect": "eslint src test --fix",
    "test": "eslint src test && mocha --opts mocha.opts test/**/*_test.js",
    "prepublish": "npm run lib",
    "lib": "babel src --out-dir lib",
    "watch": "babel -w src --out-dir lib"
  },
  "keywords": [
    "datocms",
    "gatsby"
  ],
  "author": "DatoCMS <support@datocms.com>",
  "contributors": [
    {
      "name": "Stefano Verna",
      "email": "s.verna@datocms.com"
    }
  ],
  "homepage": "https://github.com/datocms/gatsby-datocms-source",
  "bugs": {
    "url": "https://github.com/datocms/gatsby-datocms-source/issues"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-regenerator": "6.16.1",
    "babel-preset-es2015": "6.16.0",
    "chai": "3.5.0",
    "chokidar": "1.6.1",
    "dirty-chai": "1.2.2",
    "eslint": "3.7.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "^1.16.0",
    "mocha": "3.1.0",
    "nock": "8.0.0",
    "nock-vcr-recorder": "0.1.5"
  },
  "dependencies": {
    "babel-core": "^6.0.0",
    "datocms-client": "0.2.8",
    "graphql": "0.7.2",
    "humps": "1.1.0"
  }
}
